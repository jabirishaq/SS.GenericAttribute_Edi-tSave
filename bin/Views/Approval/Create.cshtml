@model SmartStore.Admin.Models.Approvals.ApprovalModel
@{
    //page title
    ViewBag.Title = "Approval Create";
}
@using (Html.BeginForm())
{
    <div class="section-header">
        <div class="title">
            Add New @Html.ActionLink("Back to list", "List")
        </div>
        <div class="options">
            <button type="submit" name="save" value="save" class="btn btn-primary">
				<i class="fa fa-check"></i>
				<span>@T("Admin.Common.Save")</span>
			</button>
            <button type="submit" name="save" value="save-continue" class="btn btn-secondary">
				<span>@T("Admin.Common.SaveContinue")</span>
			</button>
        </div>
    </div>

    Html.RenderPartial("_CreateOrUpdate", Model);
}